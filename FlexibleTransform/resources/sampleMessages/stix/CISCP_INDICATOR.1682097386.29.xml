rType">      <indicator:Type xsi:type="stixVocabs:IndicatorTypeVocab-1.0">Malicious E-mail</indicator:Type>      <indicator:Description>Spear-phishing email containing a malicious attachment.</indicator:Description>      <indicator:Observable id="NCCIC:Observable-d1e32544-dafa-4370-8a16-bde0dc093b76">        <cybox:Object id="NCCIC:Object-2bb4f4f9-9fe3-4b1d-a929-706552912ae2">          <cybox:Properties xsi:type="EmailMessageObj:EmailMessageObjectType">            <EmailMessageObj:Header>              <EmailMessageObj:Subject condition="Equals">contact</EmailMessageObj:Subject>              <EmailMessageObj:Sender category="e-mail">                <AddrObj:Address_Value condition="Equals">Dien Minh &lt;manhsanhcong12@gmail.com&gt;</AddrObj:Address_Value>              </EmailMessageObj:Sender>            </EmailMessageObj:Header>          </cybox:Properties>        </cybox:Object>      </indicator:Observable>      <indicator:Kill_Chain_Phases>        <stixCommon:Kill_Chain_Phase kill_chain_id="stix:KillChain-af3e707f-2fb9-49e5-8c37-14026ca0a5ff" kill_chain_name="LM Cyber Kill Chain" name="Delivery" ordinality="3" phase_id="stix:KillChainPhase-79a0e041-9d5f-49bb-ada4-8322622b162d"/>      </indicator:Kill_Chain_Phases>    </stix:Indicator>    <stix:Indicator id="NCCIC:indicator-d2264d08-fb72-407c-9d0c-c84a44738acf" version="2.0" xsi:type="indicator:IndicatorType">      <indicator:Type xsi:type="stixVocabs:IndicatorTypeVocab-1.0">File Hash Watchlist</indicator:Type>      <indicator:Description>This file is a web archive intended to be opened in Microsoft Word to exploit CVE-2012-0158 and ultimately drop and execute the Tigershark implant.Dropped Files:Path: %USERPROFILE%\Local Settings\Temp\M.BPath: %USERPROFILE%\Local Settings\Temp\Contact.docPath: %USERPROFILE%\Local Settings\Temp\M.T</indicator:Description>      <indicator:Observable id="NCCIC:Observable-1a3267c2-e3be-4426-afc1-cbcea57fdeb0">        <cybox:Object id="NCCIC:Object-78e06f14-e293-4be7-8721-f03d2d11a8b1"