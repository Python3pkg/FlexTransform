k78\:fvvhifvwtdv.ISZgzngpojcx.PHBzckiykotxiu.TJVvwbmun.exe</indicator:Description>      <indicator:Observable id="NCCIC:Observable-ee0742ee-f64f-4e09-bbfc-84ad3d888818">        <cybox:Object id="NCCIC:Object-cebe8fbd-3e2b-4782-98db-23309fc5bba1">          <cybox:Properties xsi:type="FileObj:FileObjectType">            <FileObj:File_Name condition="Equals">EBOLA - ETISALAT PRESENTATION.pdf.scr</FileObj:File_Name>            <FileObj:Size_In_Bytes condition="Equals">1040512 bytes</FileObj:Size_In_Bytes>            <FileObj:Hashes>              <cyboxCommon:Hash>                <cyboxCommon:Type condition="Equals" xsi:type="cyboxVocabs:HashNameVocab-1.0">MD5</cyboxCommon:Type>                <cyboxCommon:Simple_Hash_Value condition="Equals">6d7cf909bfecc26d304b20c92c1fa74e</cyboxCommon:Simple_Hash_Value>              </cyboxCommon:Hash>            </FileObj:Hashes>          </cybox:Properties>        </cybox:Object>      </indicator:Observable>      <indicator:Kill_Chain_Phases>        <stixCommon:Kill_Chain_Phase kill_chain_id="stix:KillChain-af3e707f-2fb9-49e5-8c37-14026ca0a5ff" kill_chain_name="LM Cyber Kill Chain" name="Installation" ordinality="5" phase_id="stix:KillChainPhase-e1e4e3f7-be3b-4b39-b80a-a593cfd99a4f"/>      </indicator:Kill_Chain_Phases>    </stix:Indicator>    <stix:Indicator id="NCCIC:indicator-3739f813-d6ac-4477-9efc-b98b313dc0de" version="2.0" xsi:type="indicator:IndicatorType">      <indicator:Type xsi:type="stixVocabs:IndicatorTypeVocab-1.0">File Hash Watchlist</indicator:Type>      <indicator:Description>This is an obfuscated data file related to malware installation</indicator:Description>      <indicator:Observable id="NCCIC:Observable-3333f725-e34a-412e-90cf-08fe4481d9d0">        <cybox:Object id="NCCIC:Object-cf61bc9a-52e6-4a87-912c-918bf91142f7">          <cybox:Properties xsi:type="FileObj:FileObjectType">            <FileObj:File_Name condition="Equals">fvvhifvwtdtv.IS</FileObj:File_Name>            <FileObj:Size_In_Bytes condition="Equ